 <div class="row">
  <div class="col s12 m12 l12">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title"><%= hit[:title] %></span>
        <p><%= hit[:content] %></p>
      </div>
      <div class="card-action">
        <% hit[:tests].each do |test| %>
          <a href="#"><%= test %></a>
        <% end %>
      </div>
    </div>
  </div>
</div>
            