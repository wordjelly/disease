<h5>Total diseases for which lab tests were detected: <%= @diagnoses.size %></h5>
<table>
        <thead>
          <tr>
              <th>Diagnosis Name</th>
              <th>Tests</th>
          </tr>
        </thead>
        <tbody>
			<% @diagnoses.each do |diagnosis| %>
				<%= render :partial => "diagnosis", locals: {diagnosis: diagnosis} %>
			<% end %>
		</tbody>
</table>